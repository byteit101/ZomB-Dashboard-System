<Window x:Class="System451.Communication.Dashboard.ViZ.FirstRun"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Team Number" Height="105" Width="240" Topmost="True" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen"
        Icon="pack://application:,,,/ZomB;component/Resources/ZomB.ico">
    <DockPanel Margin="10" VerticalAlignment="Center">
        <Label>Enter team number:</Label>
        <Button DockPanel.Dock="Right" TabIndex="2" Click="Button_Click">OK</Button>
        <TextBox  Name="textBox1" TabIndex="1" PreviewKeyUp="textBox1_PreviewKeyUp" />
    </DockPanel>
</Window>
