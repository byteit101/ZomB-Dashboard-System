<GroupBox x:Class="System451.Communication.Dashboard.Net.DriverStation.AxisConfigPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sys="clr-namespace:System;assembly=mscorlib" Name="Xbox">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="8" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition SharedSizeGroup="Left" Width="Auto" />
                <ColumnDefinition SharedSizeGroup="Right" Width="*" />
            </Grid.ColumnDefinitions>
            <Label>Source:</Label>
            <ComboBox Grid.Column="1" Width="200" SelectionChanged="Source_changed" Name="xsourcebox">
                <sys:String>Hardware 1</sys:String>
                <sys:String>Hardware 2</sys:String>
                <sys:String>Hardware 3</sys:String>
                <sys:String>Hardware 4</sys:String>
            </ComboBox>
            <Label Grid.Row="2">Part:</Label>
            <ComboBox Grid.Column="1" Width="200" Name="xdetailbox" Grid.Row="2">
            </ComboBox>
        </Grid>
    </GroupBox>
